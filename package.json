{
    "name": "aphelion",
    "version": "1.0.0",
    "dependencies": {
        "@fastify/autoload": "^6.3.0",
        "@fastify/cors": "^11.0.1",
        "@fastify/env": "^5.0.2",
        "@fastify/formbody": "^8.0.2",
        "@fastify/helmet": "^13.0.1",
        "@fastify/jwt": "^9.1.0",
        "@fastify/oauth2": "^8.1.2",
        "@fastify/rate-limit": "^10.3.0",
        "@fastify/sensible": "^6.0.3",
        "axios": "^1.11.0",
        "close-with-grace": "^2.2.0",
        "fastify": "^5.3.3",
        "fastify-plugin": "^5.0.1",
        "mongoose": "^8.15.0",
        "octokit": "^5.0.3",
        "pino-pretty": "^13.0.0"
    },
    "main": "src/server.js",
    "scripts": {
        "start": "node src/server.js",
        "rebuild": "docker compose build --no-cache && docker compose up -d --remove-orphans",
        "network": "docker network create aphelion-net"
    },
    "type": "module",
    "imports": {
        "#utils/*": [
            "./src/utils/*.js"
        ],
        "#constants/*": [
            "./src/constants/*.js"
        ],
        "#routes/*": [
            "./src/routes/*.js"
        ],
        "#models/*": [
            "./src/models/*.js"
        ],
        "#services/*": [
            "./src/services/*.js"
        ]
    }
}
