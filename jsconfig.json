{
    "compilerOptions": {
        "baseUrl": "src",
        "module": "nodenext",
        "moduleResolution": "nodenext",
        "paths": {
            "#utils/*": ["src/utils/*"],
            "#constants/*": ["src/constants/*"],
            "#routes/*": ["src/routes/*"],
            "#models/*": ["src/models/*"],
            "#services/*": ["src/services/*"]
        }
    },
    "include": ["src"]
}
